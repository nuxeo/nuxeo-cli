{
  "name": "nuxeo-cli",
  "version": "1.8.5-0",
  "description": "Nuxeo CLI is a command-line interface for helping developers around Nuxeo ecosystem. It provides project bootstrapping, bundles hot reload,...",
  "main": "./bin/nuxeo-cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nuxeo/nuxeo-cli"
  },
  "scripts": {
    "dev": "rm -rf $PWD/node_modules/generator-nuxeo && ln -s $PWD/../generator-nuxeo $PWD/node_modules/generator-nuxeo",
    "test": "mocha --timeout 5000 -u tdd",
    "clean": "rimraf coverage/ test-results.xml .nyc*",
    "prereport": "nyc npm test -- --reporter mocha-junit-reporter",
    "report": "nyc report",
    "coverage": "npm run report -- --reporter=cobertura",
    "preversion": "rm -rf node_modules package-lock.json && npm install --save generator-nuxeo@* && npm install && git add -f package-lock.json",
    "postversion": "npm publish && git rm -f package-lock.json && npm version --ignore-scripts --no-git-tag-version prepatch && npm install --save nuxeo/generator-nuxeo#master && git commit -am \"Post release ${npm_package_version}\" && git push && git push --tags"
  },
  "keywords": [
    "nuxeo",
    "cli"
  ],
  "author": "Arnaud Kervern",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.4"
  },
  "dependencies": {
    "ansi-256-colors": "^1.1.0",
    "chalk": "^2.0.1",
    "chokidar": "^1.7.0",
    "debug": "^2.6.8",
    "fs-extra": "^4.0.0",
    "generator-nuxeo": "github:nuxeo/generator-nuxeo#master",
    "global-tunnel-ng": "^2.1.0",
    "is-windows": "^1.0.1",
    "isarray": "^2.0.2",
    "lodash": "^4.17.4",
    "log-symbols": "^2.0.0",
    "node-machine-id": "^1.1.9",
    "pad-right": "^0.2.2",
    "parentpath": "^0.2.0",
    "semver": "^5.4.1",
    "universal-analytics": "^0.4.15",
    "update-notifier": "^2.2.0",
    "uuid": "^3.1.0",
    "yargs": "^8.0.2",
    "yeoman-environment": "^2.0.0"
  },
  "bin": {
    "nuxeo": "./bin/nuxeo-cli.js",
    "nuxeo-bootstrap": "./bin/nuxeo-bootstrap.js"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "minimatch": "^3.0.4",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.13.0",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "tmp": "0.0.31"
  }
}
